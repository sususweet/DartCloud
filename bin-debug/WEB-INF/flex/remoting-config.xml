			<source>com.cloud.resourceStatistic.ResGeoDistribution_Storage</source>
		</properties>
	</destination>
	
	<destination id="ResUtilization_ShowJobAndVMDest">
		<properties>
			<source>com.cloud.resourceStatistic.ResUtilization_ShowJobAndVM</source>
		</properties>
	</destination>
	
	<destination id="TestDest">
   		 <properties>
	        <source>com.cloud.jobManagement.Test</source>
	    </properties>
	</destination>
	
	<destination id="ImageServiceDest">
		<properties>
			<source>com.cloud.image.ImageService</source>
		</properties>
	</destination>
	
	<destination id="SessionClientDest">
   		 <properties>
	        <source>com.cloud.cda.SessionClient</source>
	    </properties>
