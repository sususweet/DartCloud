		</properties>
	</destination>
	
	<destination id="VMServiceDest">
		<properties>
			<source>com.cloud.resourceMonitor.VMService</source>
		</properties>
	</destination>
	
	<destination id="StatisticDataFetch">
   		 <properties>
	        <source>com.cloud.resourceStatistic.DataFetch</source>
