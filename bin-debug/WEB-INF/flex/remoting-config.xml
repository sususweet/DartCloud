	</destination>
	
	<destination id="VMCPUDataFetchDest">
		<properties>
			<source>com.cloud.resourceStatistic.VMCPUDataFetch</source>
		</properties>
	</destination>
	
	<destination id="VMMemoryDataFetchDest">
		<properties>
			<source>com.cloud.resourceStatistic.VMMemoryDataFetch</source>
		</properties>
	</destination>
	
	<destination id="VMStorageDataFetchDest">
		<properties>
			<source>com.cloud.resourceStatistic.VMStorageDataFetch</source>
